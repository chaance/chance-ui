{
	"name": "@chance/utils",
	"version": "0.0.12",
	"main": "./src/utils.ts",
	"module": "./src/utils.ts",
	"types": "./src/utils.ts",
	"sideEffects": false,
	"license": "MIT",
	"files": [
		"README.md",
		"LICENSE",
		"dist/**",
		"src/**"
	],
	"scripts": {
		"build": "tsup src/utils.ts --format esm,cjs --dts",
		"dev": "tsup src/utils.ts --format esm,cjs --watch --dts",
		"lint": "TIMING=1 eslint src --fix",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"devDependencies": {
		"eslint": "^8.30.0",
		"eslint-config-chance": "^2.0.2",
		"tsup": "^6.5.0",
		"typescript": "^4.9.4"
	},
	"publishConfig": {
		"access": "public",
		"main": "dist/utils.js",
		"module": "dist/utils.mjs",
		"types": "dist/utils.d.ts"
	}
}
