		"eslint": "^8.14.0",
		"eslint-config-chance": "^1.0.0",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"tsup": "^5.12.6",
		"typescript": "^4.6.4"
	},
	"peerDependencies": {
		"react": "17.x || 18.x",
		"react-dom": "17.x || 18.x"
	},
	"publishConfig": {
		"access": "public"
	}
