{
	"name": "@chance/react-utils",
	"version": "0.0.10",
	"main": "./dist/react-utils.js",
	"module": "./dist/react-utils.mjs",
	"types": "./dist/react-utils.d.ts",
	"files": [
		"README.md",
		"LICENSE",
		"dist/**",
		"src/**"
	],
	"license": "MIT",
	"scripts": {
		"build": "tsup src/react-utils.ts --format esm,cjs --dts",
