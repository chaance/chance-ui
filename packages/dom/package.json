{
	"name": "@chance/dom",
	"version": "0.0.11-pre.1",
	"main": "./dist/dom.js",
	"module": "./dist/dom.mjs",
	"types": "./dist/dom.d.ts",
	"sideEffects": false,
	"license": "MIT",
	"files": [
		"README.md",
		"LICENSE",
		"dist/**",
		"src/**"
	],
	"scripts": {
		"build": "tsup src/dom.ts --format esm,cjs --dts",
		"dev": "tsup src/dom.ts --format esm,cjs --watch --dts",
		"test": "vitest --config ../../vitest.config.ts",
		"lint": "TIMING=1 eslint src --fix",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"devDependencies": {
		"eslint": "^8.14.0",
		"eslint-config-chance": "^1.0.0",
		"tsup": "^5.12.6",
		"typescript": "^4.6.4"
	},
	"publishConfig": {
		"access": "public"
	}
}
